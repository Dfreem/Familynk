@model FamilyUnit
@inject SignInManager<FamilyMember> _signInManager;
@{
    var _userManager = _signInManager.UserManager;
    List<FamilyMember> family = _userManager.Users.ToList();

}
<div class="card">
    <div class="card-header">
        The @Model.FamilyName Family
    </div>
    <div class="card-body">
        @foreach (var member in family)
        {
            <div class="member-name">@member.Name</div>
        }
    </div>

</div>

